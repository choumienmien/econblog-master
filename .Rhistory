library(dplyr)
library(ggplot2)
library(magrittr)
knitr::opts_chunk$set(fig.showtext=TRUE)
library(showtext)
showtext_auto()
library(plotly)
FTnumbers <- read.table("/Users/Choumeinmein/Documents/GitHub/econblog-master/86_106FTnumbers.csv", header = TRUE , sep = ",")
FTnumbers %<>% mutate(year=year+1911,
ratio=round(foreigner/taiwan*100,2),
rateofchange=round(rateofchange,2))
FTnumbers %<>% filter(year>=2002)
a <- ggplot(FTnumbers, aes(year,rateofchange,size=ratio)) +
geom_point() +
labs(x="年份",
y="",
title="移工與台灣就業市場關係",
subtitle="移工較去年增減率(單位：百分比)"
)+
theme_minimal()
a
plot_ly(FTnumbers,x = ~year,y = ~rateofchange,size = ~ratio,
hoverinfo = 'text',
text = ~paste('</br>年份: ', year,
'</br> 移工的變動率: ', rateofchange,
'</br> 移工與台勞的比例: ', ratio)
) %>%
layout(showlegend = FALSE,
title='移工與台灣就業市場關係',
yaxis = list(title = "移工較去年增減率(單位：百分比)"),
xaxis = list(title = "年份")
) %>%
rangeslider()
city2 <- read.table("/Users/Choumeinmein/Documents/GitHub/econblog-master/90_106city2.csv", header = TRUE , sep = ",")
city2 %<>% filter(year>=2010)
b <- ggplot(city2,
aes(year,number, fill = city)
) +
geom_col(stat = "identity"
) +
labs(x="年份",
y="人數",
title="移工在台灣的人數",
subtitle="單位:人"
) +
scale_fill_discrete(name="縣市",
breaks=c("Taoyuan","Taichung","NewTaipei","cTaipei","cTainan","Kaohsiung"
,"aothers"),
labels=c("桃園縣","台中市","新北市","台北市","台南市","高雄市"
,"其他十四縣市")
)+
theme_minimal()
b
Taoyuan <- read.table("/Users/Choumeinmein/Documents/GitHub/econblog-master/Taoyuan.csv", header = TRUE , sep = ",")
Taoyuan %<>%
mutate(year=year+1911)%<>%
filter(year>=2012)
Taoyuan$year <- as.integer(Taoyuan$year)
c <- ggplot(Taoyuan,
aes(x=year,y=number,fill=category)
)+
geom_col()+
labs(x="年份",
y="人數",
title="桃園市產業及社服外勞人數",
subtitle="單位:人")+
scale_fill_brewer(name="產業別",
breaks=c("industry","Socialwelfare"),
labels=c("產業外籍勞工","社福外籍勞工")
)+
theme_minimal()
c
